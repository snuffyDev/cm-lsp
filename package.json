{
  "name": "svelte-language-server-web",
  "version": "0.0.7",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/snuffyDev/svelte-language-server-web"
  },
  "scripts": {
    "dev": "vite",
    "prestart": "npm run build:setup",
    "build": "node --max_old_space_size=16384 ./node_modules/tsup/dist/cli-node.js",
    "build:dev": "NODE_ENV=development node --max_old_space_size=16384 ./node_modules/tsup/dist/cli-node.js",
    "build:testing": "NODE_ENV=testing node  esbuild.js",
    "format": "prettier --write 'src/deps/svelte-language-server/src/**/*.{js,ts}' './*.{ts,cjs,js}'",
    "build:ls": "",
    "preview": "vite preview"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "devDependencies": {
    "@codemirror/state": "^6.2.1",
    "@jridgewell/trace-mapping": "^0.3.17",
    "@open-rpc/client-js": "^1.8.1",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "@types/events": "^3.0.1",
    "@types/marked": "^5.0.0",
    "@types/node": "^20.3.3",
    "@types/prismjs": "^1.26.0",
    "@typescript/vfs": "^1.4.0",
    "@vscode/emmet-helper": "2.8.4",
    "chokidar": "^3.4.1",
    "esbuild": "^0.18.15",
    "esbuild-plugin-d-ts-path-alias": "^4.2.0",
    "esbuild-plugin-resolve": "^2.0.0",
    "esbuild-plugins-node-modules-polyfill": "^1.3.0",
    "events": "^3.3.0",
    "fast-glob": "^3.3.0",
    "highlight.js": "^11.8.0",
    "less": "^4.2.0",
    "lodash-es": "^4.17.21",
    "marked": "^5.1.0",
    "marked-highlight": "^2.0.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.27",
    "postcss-load-config": "^4.0.1",
    "prettier": "2.8.6",
    "prettier-plugin-svelte": "~2.10.0",
    "pug": "^3.0.2",
    "sass": "^1.64.1",
    "scss": "^0.2.4",
    "source-map": "^0.7.4",
    "source-map-js": "^1.0.2",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "svelte2tsx": "^0.6.22",
    "tsup": "^7.2.0",
    "type-fest": "^4.3.2",
    "typescript": "^5.1.6",
    "vscode-css-languageservice": "~6.2.0",
    "vscode-html-languageservice": "^5.0.6",
    "vscode-languageserver": "8.0.2",
    "vscode-languageserver-protocol": "^3.17.3",
    "vscode-languageserver-textdocument": "^1.0.8",
    "vscode-languageserver-types": "3.17.2",
    "vscode-uri": "^3.0.7"
  },
  "overrides": {
    "estree-walker": "2.0.2",
    "svelte-preprocess": {
      "typescript": "^5.1.6"
    },
    "svelte-language-server": {
      "typescript": "5.1.6"
    },
    "periscopic": {
      "estree-walker": "2.0.2"
    },
    "typescript": "^5.1.6",
    "svelte": {
      "estree-walker": "2.0.2"
    }
  },
  "imports": {},
  "typesVersions": {
    "*": {
      "svelteWorker": [
        "dist/worker.d.ts"
      ],
      "tsWorker": [
        "dist/tsWorker.d.ts"
      ],
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./index": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./svelteWorker": {
      "require": "./dist/worker.js",
      "import": "./dist/worker.js",
      "types": "./dist/worker.d.ts"
    },
    "./tsWorker": {
      "require": "./dist/tsWorker.js",
      "import": "./dist/tsWorker.js",
      "types": "./dist/tsWorker.d.ts"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "codemirror-languageserver": "^1.11.0",
    "estree-walker": "2.0.2"
  }
}
